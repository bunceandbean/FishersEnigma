body{color:#222;background-color:#cc9;}
td{font:1em Verdana,Arial,Helvetica,sans-serif}
ul,ol{margin-left:2em}
a{color:#009}
p{line-height:1.5em;margin:.75em}
body,div,img,p,button,input,select,textarea{box-sizing:border-box}
#apple{background-image:url(https://www.theteacherscorner.net/images/navigation/sprites.png);position:absolute;top:.17em;left:.35em;z-index:100;float:left;display:block;background-position:-1px -25px;width:100px;height:108px;clear:none}
#world_famous_image{position:absolute;right:.1em;top:0}
#outer-wrap{position:relative;margin-left:auto;margin-right:auto}
#inner-wrap{height:auto;margin-left:auto;margin-right:auto;min-height:100%;width:965px;max-width:none;min-width:0;box-shadow:0 0 30px #663;border:1px solid #000}
#main_header{z-index:1;color:#fff;text-align:center;height:110px;padding-top:45px;clear:none;width:100%;min-width:0;max-width:100%;background-color:#000;position:relative;font:700 4em "Life Savers", Palatino, "Palatino Linotype", Arial, cursive;}
.main{margin-top:0;margin-left:0;clear:none;width:100%;height:100%;background-color:#fff;position:relative;top:0;left:0;float:left;padding-left:211px;margin-right:0;z-index:2}
.navigation_column{width:211px;height:100%;margin-top:0;margin-left:0;clear:both;background-color:#dfc87a;border-left:1px solid #000;position:absolute;top:0;left:0;color:#009;vertical-align:top;z-index:5;font:.75em Arial,Helvetica,sans-serif}
.content_column{float:left;height:100%;clear:none;width:100%;padding:0;border-left:1px solid #000;border-right:1px solid #000;font-size:.875em}
#pagelinks{z-index:5;position:fixed;display:none;float:left;padding:3px;top:110px;width:58px;background-color:#663;border:1px solid #000;border-radius:5px 0 0 5px;text-align:center;background-image:linear-gradient(to right,#663,#9c9c4e)}
.content_column h1{margin-top:.7em;margin-bottom:1em;padding-bottom:5px;border-bottom:1px solid #860404;border-right:1px solid #860404;border-left:1px solid #860404;padding-left:.5em;z-index:5;font:oblique 1.8em "Life Savers", Palatino, "Palatino Linotype", Garamond,cursive,Arial,Helvetica,sans-serif;text-shadow: 0px 1px 1px #888;color:red}
.content_column h2{font: 1.4em "Life Savers", Palatino, Arial,Helvetica,sans-serif;color:#03740C;}
.content_column h3{font:small-caps 1.2em Arial,Helvetica,sans-serif}
#membership_login_username {font-size:11px;width:70px;height:20px;padding:1px}
#membership_login_password {font-size:11px;width:70px;height:20px;padding:1px}
#membership_login_submit {font-size:11px}
#membership_logout_button {font-size:8px}
#breadcrumbs ul{margin:0;padding:0;list-style:none}
#breadcrumb_container{position:relative;z-index:1;width:100%;height:32px;margin-top:0;margin-left:0;clear:both;background-color:#663;border-top:1px solid #000;border-bottom:1px solid #000}
#breadcrumb_container ul{color:#fff;margin:0;padding:0;font:700 .75em Arial,Helvetica,sans-serif}
#breadcrumbs{background:#663;box-shadow:0 0 2px rgba(0,0,0,.2);overflow:hidden;width:100%}
#breadcrumbs li{float:left}
#breadcrumbs a{padding:8px 8px 8px 25px;float:left;text-decoration:none;color:#FFF;position:relative;text-shadow:0 1px 0 #333;background-color:#663;background-image:linear-gradient(to right,#663,#9c9c4e);background:-webkit-gradient(linear,left,right,from(#666633),to(#a20505));background:-webkit-linear-gradient(left,#666633,#9c9c4e);background:-moz-linear-gradient(left,#666633,#9c9c4e);background:-ms-linear-gradient(left,#666633,#9c9c4e);background:-o-linear-gradient(left,#666633,#9c9c4e);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#666633,endColorstr=#9c9c4e,GradientType=1)}
#breadcrumbs li:first-child a{padding-left:35px;border-radius:0 0 0 5px}
#breadcrumbs ul:first-child li{background:}
#breadcrumbs a:hover{background:#9c9c4e}
#breadcrumbs a::after,#breadcrumbs a::before{content:"";position:absolute;top:50%;margin-top:-1.5em;border-top:1.5em solid transparent;border-bottom:1.5em solid transparent;border-left:1em solid;right:-1em}
#breadcrumbs a::after{z-index:2;border-left-color:#9c9c4e}
#breadcrumbs a::before{border-left-color:#663;right:-1.1em;z-index:1}
#breadcrumbs a:hover::after{border-left-color:#9c9c4e}
#breadcrumbs .current,#breadcrumbs .current:hover{padding:8px 8px 8px 25px;font-weight:700;background:none}
#breadcrumbs .current::after,#breadcrumbs .current::before{content:normal}
